<mapper namespace="com.mumu.meishijia.dao.user.ITokenDao">
    <select id="queryToken" parameterType="java.lang.String" resultType="com.mumu.meishijia.pojo.user.UserToken">
        SELECT * FROM user_token WHERE token=#{token}
